{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "Auth0LockPasswordless": true
  },
  // "parser": "babel-eslint",
  "plugins": ["react"],
  "rules": {
    "arrow-body-style": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "max-len": 0,
    "no-underscore-dangle": 0
  }
}
